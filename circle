1).........................
class circle{
    constructor(radius){
        this.radius=radius;
    }
}
var C =new circle("1.0");
console.log(C.radius);
2)........................
class circle{
    constructor (radius,color){
        this.radius = radius;
        this.color = color;
    }
}
var C =new circle("1.0","red");
console.log(C); 
3)...........................
 class circle {
    constructor(radius,color){
        this.radius=radius;
        this.color=color;
    }
get circle(){
    return C.radius;
}  
  }
var C= new circle("1.0","red");
console.log(C.radius);
4)..................................
class circle{
    constructor(radius,color){
        this.radius=radius;
        this.color=color;
    }
  set changeradius(modifieradius){
   this.radius=modifieradius;
  }
}
var C =new circle("1.0","red");
C.changeradius="5.0";
console.log(C.radius);
